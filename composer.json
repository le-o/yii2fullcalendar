{
	"name": "le-o/yii2fullcalendar",
	"description": "Yii2 fullcalendar Widgets (philippfrenzel fork)",
	"keywords": ["yii2", "fullcalendar","widget"],
	"homepage": "http://www.frenzel.net/",
	"type": "yii2-extension",
	"license": "MIT",
	"authors": [
		{
			"name": "Léonard Décaillet",
			"email": "info@d-web.ch",
			"homepage": "https://www.d-web.ch",
			"role": "Developer"
		}
	],
	"support": {
		"source": "https://github.com/le-o/yii2fullcalendar"
	},
	"minimum-stability": "stable",
	"require": {
		"yiisoft/yii2": ">=2.0.14",
		"yiisoft/yii2-jui" : "^2.0",
		"fullcalendar/fullcalendar": "^3.10",
		"moment/moment" : "^2.25"
	},
	"require-dev": {
		"codeception/codeception": "*"
	},
	"config": {
	    "process-timeout": 1800,
	      "fxp-asset": {
		"repositories": {
		  "bower-asset/eve": {
		  	"type": "bower-vcs",
		  	"url": "https://github.com/adobe-webplatform/eve.git"
		  }
		}
	    }
	},
	"autoload": {
		"psr-4": { "yii2fullcalendar\\": "" }
	}
}
